    "vue-yandex-maps": "^0.12.3",
    "vue3-smooth-dnd": "^0.0.5",
    "vuedraggable": "^4.1.0",
    "xml-formatter": "^3.3.0",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@nuxt/devtools": "latest",
    "@nuxtjs/robots": "^3.0.0",
    "@types/node": "^18",
    "@vue/vue3-jest": "^29.2.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.3.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "jest": "^29.3.1",
    "lighthouse": "^9.6.8",
    "nuxt": "3.5.3",
    "nuxt-purgecss": "^2.0.0",
    "sass": "^1.32.12",
