{
  "name": "my-school-frontend",
  "version": "1.0.0",
  "description": "Цифровой помощник ученика",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=4096 nuxt build",
    "dev": "nuxt dev",
    "prod": "npm i && nuxt build && node -r dotenv/config .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npm run lint-js && npm run lint-styles",
    "lint-styles": "stylelint \"**/*.{css,scss,vue}\" --config ./stylelint.config.js",
    "lint-js": "npx eslint \"**/*.{js,vue}\" --config ./eslint.config.js",
    "test": "npm run test-units && npm run test-e2e",
    "test-units": "jest -c ./jest.unit.config.js",
    "test-e2e": "jest -c ./jest.e2e.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.services.edu.ru/student_edu_ru/frontend.git"
  },
  "overrides": {
    "pinia": {
      "vue": "^3.2.47"
    }
  },
  "dependencies": {
    "@nuxt-alt/proxy": "^2.4.3",
    "@pinia/nuxt": "^0.4.11",
    "@popperjs/core": "^2.11.6",
    "@vkontakte/superappkit": "^1.58.1",
    "@vuepic/vue-datepicker": "^3.6.5",
    "@vueuse/core": "^9.6.0",
    "add": "^2.0.6",
    "axios": "^1.2.1",
    "bootstrap": "^5.2.3",
    "chart.js": "^4.1.0",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^2.29.3",
    "dompurify": "^3.0.3",
    "dotenv": "^16.0.3",
    "gsap": "^3.11.4",
    "html-to-pdfmake": "^2.4.16",
    "http-proxy-node16": "^1.0.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.setwith": "^4.3.2",
    "lodash.shuffle": "^4.2.0",
    "mathjax-full": "^3.2.2",
    "mitt": "^3.0.0",
    "nunjucks": "^3.2.4",
    "pagination-calculator": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "pdfmake": "^0.2.7",
    "pinia": "^2.1.3",
    "puppeteer": "^20.7.1",
    "query-string": "^8.1.0",
